alias apt='sudo apt'
alias halt='sudo shutdown -h now'
alias ll='ls -lFh'
alias la='ls -lFha'
alias p='ping6'
alias q='exit'
alias reboot='sudo shutdown -r now'
alias s=sudo
alias sa='alias | fzf'
alias se='printenv | fzf'
alias stowth='stow --dotfiles --verbose --stow --target ~/'
alias svi='sudo vim'
alias tf='tail -f'
alias tt='telnet'
alias unstowth='stow --dotfiles --verbose --delete --target ~/'
alias zrc='vim ~/.zshrc'
